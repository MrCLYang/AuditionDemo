        <activity android:name=".ViewModelDemo.ViewModelActivity" />
        <activity android:name=".TestDemo.TestActivity" />
        <activity android:name=".EventBusDemo.SecondActivity" />
        <activity
            android:name=".EventBusDemo.EventBusActivity"
            android:exported="true" />
        <activity android:name=".BlueToothDemo.BleActivity" />
        <activity android:name=".SingletonModelDemo.MainActivity" />
        <activity
            android:name=".Okhttp3Demo.Okhttp3Activity"
            android:exported="true" />
        <activity
            android:name=".RetrofitDemo.RetrofitActivity"
            android:exported="true" />
        <activity
            android:name=".HandlerDemo.HandlerThreadActivity"
